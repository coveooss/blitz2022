		"sveld": "^0.10.2",
		"svelte": "^3.42.5",
		"svelte-check": "^2.0.0",
		"svelte-preprocess": "^4.0.0",
		"svelte2tsx": "^0.4.8",
		"tslib": "^2.0.0",
		"typescript": "^4.0.0",
